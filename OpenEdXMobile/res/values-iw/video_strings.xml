<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Label for the Download to sd-card setting -->
    <string name="settings_download_to_sdcard_title">הורדת הסרטון אל כרטיס SD</string>
    <!-- Label explaining the download location setting in detail -->
    <string name="settings_download_to_sdcard_subtitle">סרטונים יורדו אל כרטיס SD, אם הוא זמין</string>
    <!-- Label indicating that the only course content visible in the app is videos -->
    <string name="lbl_showing_only_videos">הצג רק סרטונים</string>
    <!-- Title of prompt indicating issue loading a video -->
    <string name="network_error_header">בעיה בטעינת סרטון</string>
    <!-- Content of prompt indicating issue loading a video -->
    <string name="network_error_message">אנא בדוק את חיבור האינטרנט שלך.</string>

    <!-- Download error messages -->
    <!-- Label shown on video list item whose download failed -->
    <string name="error_download_failed">ההורדה נכשלה</string>
    <!-- Confirmation message shown when attempting to download a large quantity of video data -->
    <string name="file_size_exceeded">גודל הקובץ להורדה גדול מהנפח הפנוי במכשיר. </string>

    <!-- Message shown when user attempts to download a video over cellular data,
    but has chosen to only allow video downloads over wifi -->
    <string name="wifi_off_message">הגדרות ההורדה הנוכחיות שלך מאפשרות הורדה רק באמצעות רשת Wi-Fi.
        \n אנא התחבר לרשת Wi-Fi או שנה את הגדרות ההורדה שלך.</string>

    <!-- Google Cast -->
    <!-- Description of a button that will cast/un-cast the app to a remote device -->
    <string name="media_route_menu_title">הפעלה ב...</string>
    <!-- Google cast Introduction title -->
    <string name="introducing_cast_text">נא להכיר: Cast (שידור)</string>
    <!-- Text to describe that video is casting on the remote device -->
    <string name="remote_device_casting_message">הסרטון משודר אל התקן מרוחק</string>

    <!-- Label explaining toggle for cellular data access -->
    <string name="wifi_dialog_message_help">אפשר למכשירך להוריד סרטונים ברשת הסלולרית כאשר Wi-Fi אינו זמין. עלולות לחול עלויות תקשורת.  </string>
    <!-- Info message text for to let use know how many videos were deleted -->
    <plurals name="delete_video_snackbar_msg">
        <item quantity="one">קובץ וידאו %s נמחק</item>
        <item quantity="two">%s קבצי וידאו נמחקו</item>
        <item quantity="many">%s קבצי וידאו נמחקו</item>
        <item quantity="other">%s קבצי וידאו נמחקו</item>
    </plurals>
</resources>
